    "paths": {
        "/cupcakes/{flavor}": {
            "post": {
                "summary": "Create a cupcake",
                "operationId": "Cupcake_Create",
                "description": "Make a cupcake.",
                "parameters": [
                    {
                        "name": "flavor",
                        "in": "path",
                        "description": "Flavor of cupcake. Ignored in test",
                        "required": true,
                        "type": "string"
                    }
                ],
                "tags": [
                    "Cupcakes"
                ],
                "responses": {
                    "201": {
                        "description": "Cupcake entity created.",
                        "schema": {
                            "$ref": "#/definitions/Cupcake"
                        }
                    },
                    "default": {
                        "description": "Error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            },
            "get": {
                "summary": "Get some cupcakes",
                "operationId": "Cupcake_Get",
                "description": "Gets all cupcakes with matching flavor",
                "parameters": [
                    {
                        "name": "flavor",
                        "in": "path",
                        "description": "Flavor of cupcake to get.",
                        "required": true,
                        "type": "string"
                    }
                ],
                "tags": [
                    "Cupcakes"
                ],
                "responses": {
                    "200": {
                        "description": "A cupcake entity with matching flavor.",
                        "type": "array",
                        "items": {
                            "$ref": "#/definitions/Cupcake"
                        }
                    },
                    "default": {
                        "description": "Error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "Cupcake": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "description": "Unique identifier"
                },
                "flavor": {
                    "type": "string",
                    "description": "Flavor"
                }
            }
        },
        "Error": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string",
                    "description": "Error code"
                },
                "message": {
                    "type": "string",
                    "description": "Message"
                }
            }
        }
    },
    "parameters": {
        "SubscriptionIdParameter": {
